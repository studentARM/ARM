}

QComboBox:hover {
    background-color: #F5B0F5;
    border-color: #BA55D3;
}

/* Стиль выпадающего списка */
QComboBox QAbstractItemView {
    background-color: #EE82EE; /* Фиолетовый фон списка */
    color: white;             /* Белый текст */
    border: 1px solid #8B008B;
    border-radius: 5px;
    selection-background-color: #BA55D3; /* Цвет выделенного пункта */
    outline: none; /* Убираем пунктирную рамку при фокусе */
}

/* Стрелка комбобокса */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: right center;
    width: 20px;
    border-left: 1px solid #8B008B;
}

QComboBox::down-arrow {
